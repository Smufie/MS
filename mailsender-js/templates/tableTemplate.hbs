<table id="person-table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Mail</th>
            <th>Interests</th>
        </tr>
    </thead>
    <tbody>
        {{#each persons}}
        <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td>{{mail}}</td>
            <td>{{#each interests}}{{interest}} {{/each}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>